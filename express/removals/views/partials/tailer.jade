- updatedAt = new Date(updatedAt)
- var delta = new Date() - updatedAt
- var fourHoursInMilliseconds = 1000 * 60 * 60 * 4
//.tailer(data-updated-at!=updatedAt.toISOString())
    if updatedAt && delta > fourHoursInMilliseconds
        .inner.warning
            span Last updated
            span.value #{moment(updatedAt).fromNow()}
    else
        .inner
            span Last updated
            span.value #{moment(updatedAt).fromNow()}

