    "date": {
      "type": "string"
    },
    "time": {
      "type": "string"
    },
    "centre": {
      "type": "string"
    },
    "operation": {
      "type": "string"
    },
    "cid_id": {
      "type": "integer"
    },
    "gender": {
      "type": "string"
    },
    "nationality": {
      "type": "string"
    },
    "bed_counts": {
      "type": "object",
      "properties": {
        "male": {
          "type": "integer"
        },
        "female": {
